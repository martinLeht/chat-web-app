{
  "name": "chat-app-server",
  "version": "1.0.0",
  "description": "Simple websocket based chat app",
  "main": "dist/server.js",
  "scripts": {
    "start:dev": "nodemon",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "start": "npm run clean:dev && tsc && node dist/server.js",
    "clean:dev": "rm -r dist/",
    "dev": "npm run clean:dev && concurrently \"npm:build:dev\" \"npm:start:dev\"",
    "dev2": "npm run clean:dev && npm run build:dev && npm run start:dev"
  },
  "author": "Martin Lehtomaa",
  "license": "ISC",
  "dependencies": {
    "@types/socket.io": "^2.1.13",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-sanitizer": "^1.0.6",
    "express-session": "^1.17.1",
    "gridfs-stream": "^1.1.1",
    "helmet": "^3.23.3",
    "inversify": "^5.1.1",
    "inversify-express-utils": "^6.3.2",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "mongo-uuid": "^1.1.0",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.5",
    "mongoose-sequence": "^5.3.1",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^3.3.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.0.1",
    "uuid-mongodb": "^2.4.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/helmet": "0.0.47",
    "@types/inversify": "^2.0.33",
    "@types/mongodb": "^3.6.12",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^14.14.41",
    "tslint": "^6.1.3"
  }
}
